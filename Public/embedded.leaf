<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>SwiftFiddle - Swift Online Playground</title>
</head>

<body>
  <div class="container-fluid g-0 d-flex flex-column min-vh-100">
    <div class="row bg-light flex-fill fill d-flex g-0">
      <div class="col-md-6">
        <div id="editor-container" class="h-100"></div>
      </div>
      <div class="col-md-6">
        <div id="terminal-container" class="h-100" style="position: relative;">
          <div class="run-button-background">
            <button type="button" id="run-button" class="btn btn-link">
              <span id="run-button-icon" class="fas fa-play fa-sm fa-fw"></span>
              <span id="run-button-spinner" class="fas fa-circle-notch fa-spin fa-sm fa-fw d-none"></span>
            </button>
          </div>
          <div class="open-button-background">
            <a id="open-button" class="btn btn-link" href="#(url)" target="_blank" rel="noopener">
              <span class="fas fa-external-link-alt fa-sm fa-fw pe-1"></span>
            </a>
          </div>
        </div>
      </div>
    </div>

    <div class="row d-none">
      <div class="col">
        <span class="dropdown">
          <button class="btn btn-outline-primary btn-sm rounded-pill dropdown-toggle w-100" type="button"
            data-bs-toggle="dropdown" aria-expanded="false" id="version-selector">
            <span class="fas fa-code-branch mx-2" style="width: 1em; height: 1em;"></span>
            <span id="version-value" class="fw-bolder">#(stableVersion)</span>
          </button>
          <!-- beautify ignore:start -->
          <ul id="version-picker" class="dropdown-menu">
            #for(versionGroup in versions):
              <li><h6 class="dropdown-header">#(versionGroup.majorVersion)</h6></li>
              #for(version in versionGroup.versions):
                #if(version == stableVersion):
                  <li class="version-picker-item ms-2 active-tick"><a class="dropdown-item" data-target="\#">#(version)</a></li>
                #else:
                  <li class="version-picker-item ms-2"><a class="dropdown-item" data-target="\#">#(version)</a></li>
                #endif
              #endfor
            #endfor
          </ul>
          <!-- beautify ignore:end -->
        </span>
      </div>
    </div>
  </div>

  <!-- beautify ignore:start -->
  <script>
    const foldingRanges = [];
    #for(range in foldRanges):
    foldingRanges.push({ start: #(range.start), end: #(range.end) });
    #endfor
    
    window.appConfig = {
      initialText: `#(codeSnippet)`,
      isEmbedded: true,
      foldingRanges: foldingRanges,
      timeout: null,
      compilerOptions: null,
    }
  </script>
  <!-- beautify ignore:end -->
</body>

</html>